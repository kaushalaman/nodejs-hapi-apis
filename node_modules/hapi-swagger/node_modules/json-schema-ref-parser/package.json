{
  "name": "json-schema-ref-parser",
  "version": "3.1.2",
  "description": "Parse, Resolve, and Dereference JSON Schema $ref pointers",
  "keywords": [
    "json",
    "schema",
    "jsonschema",
    "json-schema",
    "json-pointer",
