    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.3.0",
    "karma-sinon": "^1.0.4",
    "karma-verbose-reporter": "0.0.3",
    "mocha": "^2.4.5",
    "npm-check-updates": "^2.5.8",
    "phantomjs": "^2.1.3",
    "simplifyify": "^2.0.1",
    "sinon": "^1.17.3",
    "version-bump-prompt": "^1.5.2"
